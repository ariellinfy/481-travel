@page "/explore"
@layout PageLayout
@inject NavigationManager nav
@inject PageHistoryState PageHistoryState

<PageTitle>Explore</PageTitle>

<div class="page-container">
    <nav class="navbar navbar-light">
        <div class="input-group mb-2">
            <input class="form-control mr-sm-2" type="search" placeholder="Where would you like to go?" aria-label="Search"
                   @onfocus="onSearch" @oninput="(e) => Searching(e.Value.ToString())">
            <button type="button" class="navbar-toggler" data-bs-toggle="modal" data-bs-target="#amenity-search-filter">
                <span class="navbar-toggler-icon"></span>
            </button>
            @if (state.OnSearch == true)
            {
                <button class="btn btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false"></button>
                <ul class="dropdown-menu dropdown-menu-end dropdown-menu-dark">
                    <li><a class="dropdown-item" href="#">Best Match</a></li>
                    <li><a class="dropdown-item" href="#">Most Popular</a></li>
                    <li><a class="dropdown-item" href="#">Newly Added</a></li>
                </ul>
            }
        </div>
    </nav>

    @if (state.OnSearch == false) 
    {
        <h4>Popular Places</h4>

        <div class="list-container">
            @if (state.PopularAmenities != null)
            {
                @foreach (var popularPlace in state.PopularAmenities)
                {
                    <AmenityCard amenityId="@popularPlace.id" imgUrl="@popularPlace.ImgUrl"
                         amenityName="@popularPlace.Name" amenityAddress="@popularPlace.Address" />
                }
            }
        </div>

        <h4>Suggested Places</h4>

        <div class="list-container">
            @if (state.SuggestedAmenities != null)
            {
                @foreach (var suggestedPlace in state.SuggestedAmenities)
                {
                    <AmenityCard amenityId="@suggestedPlace.id" imgUrl="@suggestedPlace.ImgUrl"
                     amenityName="@suggestedPlace.Name" amenityAddress="@suggestedPlace.Address" />
                }
            }
        </div>

        <h4>Categories</h4>

        <div class="list-container">
            <div @onclick="CateShoppingFilter">
                <CategoryCard imgUrl="https://www.incimages.com/uploaded_files/image/1920x1080/getty_522735456_249841.jpg" categoryName="Shopping" />
            </div>
            <div @onclick="CateArtFilter">
                <CategoryCard imgUrl="data:image/jpeg;base64,"
                      categoryName="Art & Culture"/>
            </div>
            <div @onclick="CateGameFilter">
                <CategoryCard imgUrl="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQZUdemxuuMcmPO6nnC90ffk4EuDHf7dwE2Tw&usqp=CAU" categoryName="Games" />
            </div>
            <div @onclick="CateFoodFilter">
                <CategoryCard imgUrl="data:image/jpeg;base64,"
                      categoryName="Food" />
            </div>
            <div @onclick="CateNightFilter">
                <CategoryCard imgUrl="https://i0.wp.com/stanzaliving.wpcomstaging.com/wp-content/uploads/2022/04/8d5f9-shutterstock_1436812790.jpg?fit=1000%2C667&ssl=1" categoryName="Night Life" />
            </div>
            <div @onclick="CateSportFilter">
                <CategoryCard imgUrl="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQH3bbdFzjKOZYl5kfsRLI92WF1h6pAd8n6zQ&usqp=CAU" categoryName="Sports" />
            </div>
        </div>
    }
    else 
    {
        <hr />
        @if (searchResults.Count > 0)
        {
            @foreach (var result in searchResults)
            {
                <AmenitySearchResultCard TargetAmenity="@result" />
                <hr />
            }
        }
        else
        {
            <div class="empty-result">Sorry, no amenity match your search...</div>
        }
    }
</div>

@code {
    [CascadingParameter]
    public StateModel state { get; set; }

    // for filter
    private int ratingValue = 3;
    private bool isArt = false;
    private bool isShopping = false;
    private bool isGame = false;
    private bool isFood = false;
    private bool isMuseum = false;
    private bool isNight = false;
    private bool isHistoric = false;
    private bool isWild = false;
    private bool isParks = false;
    private bool isSport = false;
    private string location = "";

    // for search bar
    private string searchKeyword = "";
    private HashSet<Amenity> searchResults = new HashSet<Amenity> { };
    private List<string> filters = new List<string> { };

    private void onSearch()
    {
        state.OnSearch = true;    
    }

    protected override void OnInitialized()
    {
        state.OnSearch = false;
        PageHistoryState.AddPageToHistory("/explore");
        base.OnInitialized();
    }

    private void ArtToggle() 
    {
        isArt = !isArt;
    }

    private void ShoppingToggle() 
    {
        isShopping = !isShopping;
    }

    private void GameToggle() 
    {
        isGame = !isGame;
    }

    private void FoodToggle() 
    {
        isFood = !isFood;
    }

    private void MuseumToggle() 
    {
        isMuseum = !isMuseum;
    }

    private void NightToggle() 
    {
        isNight = !isNight;
    }

    private void HistoricToggle()
    {
        isHistoric = !isHistoric;
    }

    private void WildToggle() 
    {
        isWild = !isWild;
    }

    private void ParkToggle()
    {
        isParks = !isParks;
    }

    private void SportToggle()
    {
        isSport = !isSport;
    }

    private void LocationAnyToggle()
    {
        location = "";
    }

    private void IndoorToggle()
    {
        location = "indoor";
    }

    private void OutdoorToggle()
    {
        location = "outdoor";
    }

    private void CateShoppingFilter()
    {
        onSearch();
        isShopping = true;
        ApplyFilters();
    }

    private void CateArtFilter()
    {
        onSearch();
        isArt = true;
        ApplyFilters();
    }

    private void CateGameFilter()
    {
        onSearch();
        isGame = true;
        ApplyFilters();
    }

    private void CateFoodFilter()
    {
        onSearch();
        isFood = true;
        ApplyFilters();
    }

    private void CateNightFilter()
    {
        onSearch();
        isNight = true;
        ApplyFilters();
    }

    private void CateSportFilter()
    {
        onSearch();
        isSport = true;
        ApplyFilters();
    }

    private void ApplyFilters()
    {
        onSearch();
        filters.Clear();
        if (isArt) filters.Add("art & culture");
        if (isShopping) filters.Add("shopping");
        if (isGame) filters.Add("fun & games");
        if (isFood) filters.Add("food & beverage");
        if (isMuseum) filters.Add("museum");
        if (isNight) filters.Add("nightlife");
        if (isHistoric) filters.Add("historic");
        if (isWild) filters.Add("wildlife");
        if (isParks) filters.Add("parks");
        if (isSport) filters.Add("sports");
        if (location.Length > 0) filters.Add(location);
        Filtering();
    }

    private void Filtering()
    {
        searchResults.Clear();
        if (searchKeyword.Length > 0)
        {
            Searching(searchKeyword);
            searchResults = searchResults.Where(p => p.Rating >= ratingValue).ToHashSet();
            if (filters.Count > 0)
            {
                foreach (string filter in filters)
                {
                    searchResults = searchResults.Where(p => p.Categories.Contains(filter)).ToHashSet();
                    state.SuggestedAmenities = searchResults.ToList();
                }
            }
        }
        else
        {
            foreach (string filter in filters)
            {
                Console.WriteLine(filter);
            }
            
            searchResults = state.AllAmenities.Where(p => p.Rating >= ratingValue).ToHashSet();
            Console.WriteLine(searchResults.Count);
            if (filters.Count > 0)
            {
                foreach (string filter in filters)
                {
                    searchResults = searchResults.Where(p => p.Categories.Contains(filter)).ToHashSet();
                    state.SuggestedAmenities = searchResults.ToList();
                }
            }
        }
    }

    private void Searching(string searchText)
    {
        searchKeyword = searchText;
        searchText = searchText.Trim();
        if (searchText.Length > 0)
        {
            searchResults = state.AllAmenities.Where(p => p.Name.Contains(searchText, System.StringComparison.CurrentCultureIgnoreCase)).ToHashSet();
            searchResults.UnionWith(state.AllAmenities.Where(p => p.Tags[0].Contains(searchText)).ToHashSet());
            state.SuggestedAmenities = searchResults.ToList();
            base.OnParametersSet();
        }
        else
        {
            searchResults.Clear();
        }
    }
}

<div class="modal fade" id="amenity-search-filter" tabindex="-1" aria-labelledby="amenitySearchFilterModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <FilterHeader />
            </div>
            <form>
                <div class="modal-body">
                    <h5>Category (Select one or more!)</h5>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isArt" id="cate-art" autocomplete="off" @onclick="ArtToggle">
                            <label class="btn btn-filter-category" for="cate-art">Art & Culture</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isShopping" id="cate-shopping" autocomplete="off" @onclick="ShoppingToggle">
                            <label class="btn btn-filter-category" for="cate-shopping">Shopping</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isGame" id="cate-games" autocomplete="off" @onclick="GameToggle">
                            <label class="btn btn-filter-category" for="cate-games">Fun & Games</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isFood" id="cate-food" autocomplete="off" @onclick="FoodToggle">
                            <label class="btn btn-filter-category" for="cate-food">Food & Beverage</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isMuseum" id="cate-museums" autocomplete="off" @onclick="MuseumToggle">
                            <label class="btn btn-filter-category" for="cate-museums">Museums</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isNight" id="cate-nightlife" autocomplete="off" @onclick="NightToggle">
                            <label class="btn btn-filter-category" for="cate-nightlife">Night Life</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isHistoric" id="cate-historic" autocomplete="off" @onclick="HistoricToggle">
                            <label class="btn btn-filter-category" for="cate-historic">Historic Sites</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isWild" id="cate-wildlife" autocomplete="off" @onclick="WildToggle">
                            <label class="btn btn-filter-category" for="cate-wildlife">Wildlife</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isParks" id="cate-parks" autocomplete="off" @onclick="ParkToggle">
                            <label class="btn btn-filter-category" for="cate-parks">Parks</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="@isSport" id="cate-sports" autocomplete="off" @onclick="SportToggle">
                            <label class="btn btn-filter-category" for="cate-sports">Sports</label><br>
                        </div>
                    </div>
                    <br />
                    <div class="rating-header">
                        <h5>Rating</h5>
                        <RadzenText TextStyle="TextStyle.Subtitle2" TagName="TagName.H3">Over @ratingValue</RadzenText>
                    </div>
                        <RadzenSlider @bind-Value=@ratingValue TValue="int" Min="1" Max="5" Step="1" Class="w-100" />
                    <div class="tickmarks">
                        <div>1+</div>
                        <div>2+</div>
                        <div>3+</div>
                        <div>4+</div>
                        <div>5</div>
                    </div>
                    <br />
                    <h5>Location</h5>
                    <div class="row">
                        <div class="col">
                            <input type="radio" class="btn-check" name="location" value="any" id="location-any" autocomplete="off" checked @onclick="LocationAnyToggle">
                            <label class="btn btn-filter-location" for="location-any">Any</label>
                        </div>
                        <div class="col">
                            <input type="radio" class="btn-check" name="location" value="indoor" id="location-indoor" autocomplete="off" @onclick="IndoorToggle">
                            <label class="btn btn-filter-location" for="location-indoor">Indoor</label>
                        </div>
                        <div class="col">
                            <input type="radio" class="btn-check" name="location" value="outdoor" id="location-outdoor" autocomplete="off" @onclick="OutdoorToggle">
                            <label class="btn btn-filter-location" for="location-outdoor">Outdoor</label>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary-filled" data-bs-dismiss="modal" @onclick="ApplyFilters">Apply</button>
                </div>
            </form>
        </div>
    </div>
</div>