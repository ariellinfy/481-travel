@page "/events"
@layout PageLayout
@inject NavigationManager nav
@inject PageHistoryState PageHistoryState

<PageTitle>Events</PageTitle>

<div class="page-container">
    <nav class="navbar navbar-light">
        <div class="input-group mb-2">
            <input class="form-control mr-sm-2" type="search" placeholder="Search upcoming events" aria-label="Search"
                   @onfocus="onSearch" @oninput="(e) => Searching(e.Value.ToString())">
            <button type="button" class="navbar-toggler" data-bs-toggle="modal" data-bs-target="#event-search-filter">
                <span class="navbar-toggler-icon"></span>
            </button>
            @if (state.OnSearch == true)
            {
                <button class="btn btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false"></button>
                <ul class="dropdown-menu dropdown-menu-end dropdown-menu-dark">
                    <li><a class="dropdown-item" href="#">Best Match</a></li>
                    <li><a class="dropdown-item" href="#">Most Popular</a></li>
                    <li><a class="dropdown-item" href="#">Closest Date</a></li>
                </ul>
            }
        </div>
    </nav>

    @if (state.OnSearch == false)
    {
        <h4>Popular Events</h4>

        <div class="list-container">
            @if (state.PopularEvents != null)
            {
                @foreach (var popularEvent in state.PopularEvents)
                {
                    <EventCard eventId="@popularEvent.id" imgUrl="@popularEvent.ImgUrl"
                   eventName="@popularEvent.Name" eventDate="@popularEvent.Date" eventStart="@popularEvent.StartTime" eventEnd="@popularEvent.EndTime" />
                }
            }
        </div>

        <h4>Suggested Events</h4>

        <div class="list-container">
            @if (state.SuggestedEvents != null)
            {
                @foreach (var suggestedEvent in state.SuggestedEvents)
                {
                    <EventCard eventId="@suggestedEvent.id" imgUrl="@suggestedEvent.ImgUrl"
                   eventName="@suggestedEvent.Name" eventDate="@suggestedEvent.Date" eventStart="@suggestedEvent.StartTime" eventEnd="@suggestedEvent.EndTime" />
                }
            }
        </div>

        <h4>Categories</h4>

        <div class="list-container">
            <div @onclick="CateDancingFilter">
                <CategoryCard imgUrl="data:image/jpeg;base64," 
                    categoryName="Dancing" />
            </div>
            <div @onclick="CateGameFilter">
                <CategoryCard imgUrl="data:image/jpeg;base64," 
                    categoryName="Games" />
            </div>
            <div @onclick="CateArtFilter">
                <CategoryCard imgUrl="data:image/jpeg;base64,"
                          categoryName="Art & Culture" />
            </div>
            <div @onclick="CateFoodFilter">
                <CategoryCard imgUrl="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS9jRptXnVlRftWaawl07hCtxjas3Zy7kxq-g&usqp=CAU"
                      categoryName="Food" />
            </div>
            <div @onclick="CateTechFilter">
                <CategoryCard imgUrl="data:image/jpeg;base64," 
                    categoryName="Technology" />
            </div>
            <div @onclick="CateTravelFilter">
                <CategoryCard imgUrl="data:image/jpeg;base64," 
                    categoryName="Travel" />
            </div>
        </div>
    }
    else
    {
        <hr />
        @if (searchResults.Count > 0)
        {
            @foreach (var result in searchResults)
            {
                <EventSearchResultCard TargetEvent="@result" />
                <hr />
            }
        }
        else
        {
            <div class="empty-result">Sorry, no event match your search...</div>
        }
    }
</div>

@code {
    [CascadingParameter]
    public StateModel state { get; set; }

    // for filter
    private bool isArt = false;
    private bool isDancing = false;
    private bool isGame = false;
    private bool isHobby = false;
    private bool isLanguage = false;
    private bool isFood = false;
    private bool isTech = false;
    private bool isWriting = false;
    private bool isTravel = false;
    private bool isSport = false;
    private string venue = "";

    // for search bar
    private string searchKeyword = "";
    private HashSet<Event> searchResults = new HashSet<Event> { };
    private List<string> filters = new List<string> { };

    private void onSearch()
    {
        state.OnSearch = true;
    }

    protected override void OnInitialized()
    {
        state.OnSearch = false;
        PageHistoryState.AddPageToHistory("/events");
        base.OnInitialized();
    }

    private void ArtToggle()
    {
        isArt = !isArt;
    }

    private void DancingToggle()
    {
        isDancing = !isDancing;
    }

    private void GameToggle()
    {
        isGame = !isGame;
    }

    private void HobbyToggle()
    {
        isHobby = !isHobby;
    }

    private void LanguageToggle()
    {
        isLanguage = !isLanguage;
    }

    private void FoodToggle()
    {
        isFood = !isFood;
    }

    private void TechToggle()
    {
        isTech = !isTech;
    }

    private void WritingToggle()
    {
        isWriting = !isWriting;
    }

    private void TravelToggle()
    {
        isTravel = !isTravel;
    }

    private void SportToggle()
    {
        isSport = !isSport;
    }

    private void VenueAnyToggle()
    {
        venue = "";
    }

    private void IndoorToggle()
    {
        venue = "indoor";
    }

    private void OutdoorToggle()
    {
        venue = "outdoor";
    }

    private void CateDancingFilter()
    {
        onSearch();
        isArt = false;
        isDancing = true;
        isGame = false;
        isHobby = false;
        isLanguage = false;
        isFood = false;
        isTech = false;
        isWriting = false;
        isTravel = false;
        isSport = false;
        ApplyFilters();
    }

    private void CateGameFilter()
    {
        onSearch();
        isArt = false;
        isDancing = false;
        isGame = true;
        isHobby = false;
        isLanguage = false;
        isFood = false;
        isTech = false;
        isWriting = false;
        isTravel = false;
        isSport = false;
        ApplyFilters();
    }

    private void CateArtFilter()
    {
        onSearch();
        isArt = true;
        isDancing = false;
        isGame = false;
        isHobby = false;
        isLanguage = false;
        isFood = false;
        isTech = false;
        isWriting = false;
        isTravel = false;
        isSport = false;
        ApplyFilters();
    }

    private void CateFoodFilter()
    {
        onSearch();
        isArt = false;
        isDancing = false;
        isGame = false;
        isHobby = false;
        isLanguage = false;
        isFood = true;
        isTech = false;
        isWriting = false;
        isTravel = false;
        isSport = false;
        ApplyFilters();
    }

    private void CateTechFilter()
    {
        onSearch();
        isArt = false;
        isDancing = false;
        isGame = false;
        isHobby = false;
        isLanguage = false;
        isFood = false;
        isTech = true;
        isWriting = false;
        isTravel = false;
        isSport = false;
        ApplyFilters();
    }

    private void CateTravelFilter()
    {
        onSearch();
        isArt = false;
        isDancing = false;
        isGame = false;
        isHobby = false;
        isLanguage = false;
        isFood = false;
        isTech = false;
        isWriting = false;
        isTravel = true;
        isSport = false;
        ApplyFilters();
    }

    private void ApplyFilters()
    {
        onSearch();
        filters.Clear();
        if (isArt) filters.Add("art & culture");
        if (isDancing) filters.Add("dancing");
        if (isGame) filters.Add("games");
        if (isHobby) filters.Add("hobbies");
        if (isLanguage) filters.Add("language");
        if (isFood) filters.Add("food");
        if (isTech) filters.Add("technology");
        if (isWriting) filters.Add("writing");
        if (isTravel) filters.Add("travel");
        if (isSport) filters.Add("sports");
        if (venue.Length > 0) filters.Add(venue);
        Filtering();
    }

    private void Filtering()
    {
        searchResults.Clear();
        if (searchKeyword.Length > 0)
        {
            Searching(searchKeyword);
            if (filters.Count > 0)
            {
                foreach (string filter in filters)
                {
                    searchResults = searchResults.Where(p => p.Categories.Contains(filter)).ToHashSet();
                    state.SuggestedEvents = searchResults.ToList();
                }
            }
        }
        else
        {
            if (filters.Count > 0)
            {
                searchResults = state.AllEvents.Where(p => p.Categories.Contains(filters[0])).ToHashSet();
                foreach (string filter in filters)
                {
                    searchResults = searchResults.Where(p => p.Categories.Contains(filter)).ToHashSet();
                    state.SuggestedEvents = searchResults.ToList();
                }
            }
        }
    }

    private void Searching(string searchText)
    {
        searchKeyword = searchText;
        searchText = searchText.Trim();
        if (searchText.Length > 0)
        {
            if (searchResults.Count > 0)
            {
                searchResults = searchResults.Where(p => p.Name.Contains(searchText, System.StringComparison.CurrentCultureIgnoreCase)).ToHashSet();
                state.SuggestedEvents = searchResults.ToList();
            }
            else
            {
                searchResults = state.AllEvents.Where(p => p.Name.Contains(searchText, System.StringComparison.CurrentCultureIgnoreCase)).ToHashSet();
                searchResults.UnionWith(state.AllEvents.Where(p => p.Tags.Contains(searchText)).ToHashSet());
                state.SuggestedEvents = searchResults.ToList();
            }
            
            base.OnParametersSet();
        }
        else
        {
            searchResults.Clear();
        }
    }
}

<div class="modal fade" id="event-search-filter" tabindex="-1" aria-labelledby="eventSearchFilterModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <FilterHeader />
            </div>
            <form>
                <div class="modal-body">
                    <h5>Starting Time</h5>
                    <div class="row">
                        <div class="col">
                            <input type="radio" class="btn-check" name="start_time" value="today" id="start_today" autocomplete="off">
                            <label class="btn btn-filter-start" for="start_today">Today</label>
                        </div>
                        <div class="col">
                            <input type="radio" class="btn-check" name="start_time" value="tomorrow" id="start_tomorrow" autocomplete="off">
                            <label class="btn btn-filter-start" for="start_tomorrow">Tomorrow</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="radio" class="btn-check" name="start_time" value="this weekend" id="start_this_weekend" autocomplete="off">
                            <label class="btn btn-filter-start" for="start_this_weekend">This Weekend</label>
                        </div>
                        <div class="col">
                            <input type="radio" class="btn-check" name="start_time" value="next week" id="start_next_week" autocomplete="off">
                            <label class="btn btn-filter-start" for="start_next_week">Next Week</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="radio" class="btn-check" name="start_time" value="next weekend" id="start_next_weekend" autocomplete="off">
                            <label class="btn btn-filter-start" for="start_next_weekend">Next Weekend</label>
                        </div>
                        <div class="col">
                            <input type="radio" class="btn-check" name="start_time" value="all" id="start_all_upcoming" autocomplete="off" checked>
                            <label class="btn btn-filter-start" for="start_all_upcoming">All Upcoming</label>
                        </div>
                    </div>
                    <br />
                    <h5>Category (Select one or more!)</h5>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="art" id="cate-art" autocomplete="off" @onclick="ArtToggle">
                            <label class="btn btn-filter-category" for="cate-art">Art & Culture</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="dancing" id="cate-dancing" autocomplete="off" @onclick="DancingToggle">
                            <label class="btn btn-filter-category" for="cate-dancing">Dancing</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="games" id="cate-games" autocomplete="off" @onclick="GameToggle">
                            <label class="btn btn-filter-category" for="cate-games">Games</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="hobbies" id="cate-hobbies" autocomplete="off" @onclick="HobbyToggle">
                            <label class="btn btn-filter-category" for="cate-hobbies">Hobbies</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="language" id="cate-language" autocomplete="off" @onclick="LanguageToggle">
                            <label class="btn btn-filter-category" for="cate-language">Language</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="food" id="cate-food" autocomplete="off" @onclick="FoodToggle">
                            <label class="btn btn-filter-category" for="cate-food">Food</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="technology" id="cate-tech" autocomplete="off" @onclick="TechToggle">
                            <label class="btn btn-filter-category" for="cate-tech">Technology</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="writing" id="cate-writing" autocomplete="off" @onclick="WritingToggle">
                            <label class="btn btn-filter-category" for="cate-writing">Writing</label><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="travel" id="cate-travel" autocomplete="off" @onclick="TravelToggle">
                            <label class="btn btn-filter-category" for="cate-travel">Travel</label><br>
                        </div>
                        <div class="col">
                            <input type="checkbox" class="btn-check" value="sports" id="cate-sports" autocomplete="off" @onclick="SportToggle">
                            <label class="btn btn-filter-category" for="cate-sports">Sports</label><br>
                        </div>
                    </div>
                    <br />
                    <h5>Venue Type</h5>
                    <div class="row">
                        <div class="col">
                            <input type="radio" class="btn-check" name="venue" value="any" id="venue-any" autocomplete="off" checked @onclick="VenueAnyToggle">
                            <label class="btn btn-filter-location" for="venue-any">Any</label>
                        </div>
                        <div class="col">
                            <input type="radio" class="btn-check" name="venue" value="indoor" id="venue-indoor" autocomplete="off" @onclick="IndoorToggle">
                            <label class="btn btn-filter-location" for="venue-indoor">Indoor</label>
                        </div>
                        <div class="col">
                            <input type="radio" class="btn-check" name="venue" value="outddoor" id="venue-outdoor" autocomplete="off" @onclick="OutdoorToggle">
                            <label class="btn btn-filter-location" for="venue-outdoor">Outdoor</label>
                        </div>
                    </div>
                </div>
                <div class="modal-footer filter-controls">
                    <button class="btn btn-secondary-filled" type="button" data-bs-dismiss="modal" aria-label="Close">Cancel</button>
                    <button type="button" class="btn btn-primary-filled" data-bs-dismiss="modal" @onclick="ApplyFilters">Apply</button>
                </div>
            </form>
        </div>
    </div>
</div>